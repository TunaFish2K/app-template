{
  "name": "template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./src/main/main.ts",
  "license": "MIT",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vue-tsc -p tsconfig/tsconfig.vite.json && vite build",
    "vite:preview": "vite preview",
    "dev": "APP_DEVELOPMENT=development concurrently --kill-others \"bun vite:dev\" \"wait-on http://localhost:5173 && bun run ./src/main/index.ts\"",
    "manifest": "bun vite:build && bun build.ts manifest",
    "build": "bun run vite:build && bun run manifest && bun build.ts build"
  },
  "dependencies": {
    "lucide-vue-next": "^0.562.0",
    "webview-bun": "exoRift/webview-bun"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.23",
    "bun-types": "^1.3.5",
    "concurrently": "^9.2.1",
    "eruda": "^3.4.3",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue": "^3.5.26",
    "vue-router": "v4",
    "vue-tsc": "^3.1.4",
    "wait-on": "^9.0.3"
  }
}